{
  "name": "quaint-look-nice",
  "description": "A simple CSS sheet for use with Quaint. It looks nice!",
  "version": "0.0.0",
  "format": "cjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/breuleux/quaint-look-nice"
  },
  "keywords": [
    "quaint",
    "quaint-plugin",
    "css",
    "stylesheet"
  ],
  "author": "Olivier Breuleux",
  "license": "MIT",
  "main": "./lib/index.js",
  "dependencies": {
    "deepmerge": "^0.2.10",
    "earlgrey-runtime": ">=0.0.11",
    "node-sass": "^3.4.2",
    "quaint-google-fonts": ">=0.0.0",
    "quaint-nav": ">=0.0.0"
  },
  "devDependencies": {
    "earlgrey": ">=0.0.13",
    "mocha": "^2.2.1",
    "earl-mocha": ">=0.0.3"
  },
  "scripts": {
    "refresh": "earl compile -5vso lib/ src/",
    "compile": "earl compile -5rvso lib/ src/",
    "prepublish": "npm run compile",
    "test": "mocha"
  }
}
